<label class="margin-top-10">Name</label>
<%= f.text_field(:name,{placeholder:'Ex : Sushi ..',class:'form-control'}) %>

<label class="margin-top-10">Category</label>
<%= f.select(:category,@categories,{},{class:'form-control'}) %>

<label class="margin-top-10">Price</label>
<%= f.text_field(:price,{class:'form-control'}) %>

<label class="margin-top-10">Available</label>
<%= f.check_box(:available) %><br>

<label class="margin-top-10">Delivery Time (in hr.)</label>
<%= f.text_field(:delivery_time,{class:'form-control'}) %>




